				"type": "string"
			},
			{
				"name": "source_dirs",
				"value": "src/main/java",
				"type": "string"
			},
			{
				"name": "test_dirs",
				"value": "src/test/java",
				"type": "string"
			}
		],
		"deployment_files": [],
		"application_init": {
			"command": "",
